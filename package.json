    "ali-oss": "^6.17.1",
    "ant-design-vue": "3.2.0",
    "axios": "^0.26.1",
    "cropperjs": "^1.5.12",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.1",
    "json-big": "^1.0.2",
    "lodash-es": "^4.17.21",
    "mime": "^3.0.0",
    "mockjs": "^1.1.0",
